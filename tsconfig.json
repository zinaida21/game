{
    "compilerOptions": {
        "strict": true, // Включает все жесткие проверки типов
        "outDir": "./dist/",
        "noImplicitAny": true, //true  typescript допускает параметры без объявления их типа
        "module": "es6",
        "target": "es5",
        "allowJs": true,
        "sourceMap": true,
        "moduleResolution": "node",
        "downlevelIteration": true,
        "checkJs": true,
        "strictNullChecks": false
    }
}
